    "@react-navigation/native": "^7.0.0",
    "cbl-reactnative": "file:..",
    "expo": "^52.0.11",
    "expo-dev-client": "~5.0.4",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-random": "~14.0.1",
    "expo-router": "~4.0.9",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.4",
    "expo-web-browser": "~14.0.1",
    "react": "18.3.1",
