{
  "name": "expo-example",
  "main": "expo-router/entry",
  "version": "0.2.2",
  "scripts": {
    "clean": "npm run prebuild:clean",
    "prebuild": "cd .. && npm run build && cd expo-example && npx expo prebuild",
    "prebuild:clean": "cd .. && npm run build && cd expo-example && npx expo prebuild --clean",
    "start": "npx expo start",
    "android": "cd .. && npm run build && cd expo-example && npx expo run:android",
    "ios": "cd .. && npm run build && cd expo-example && npx expo run:ios",
    "test": "jest --watchAll",
    "verify": "npm run prebuild"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gluestack-style/react": "^1.0.57",
